{
  "kind": "collectionType",
  "collectionName": "team_colors",
  "info": {
    "singularName": "team-color",
    "pluralName": "team-colors",
    "displayName": "TeamColor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "value": {
      "type": "string",
      "regex": "^#(?:[0-9a-fA-F]{3}){1,2}$"
    },
    "teams": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::team.team",
      "mappedBy": "team_colors"
    }
  }
}
